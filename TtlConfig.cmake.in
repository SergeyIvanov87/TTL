
# - Config file for the Ttl package
# It defines the following variables
#  TTL_INCLUDE_DIRS - include directories for Ttl
#  TTL_LIBRARIES    - libraries to link against
#  TTL_EXECUTABLE   - the bar executable

#  Set paths
get_filename_component(TTL_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(TTL_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# Our library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET Ttl AND NOT Ttl_BINARY_DIR)
  include("${TTL_CMAKE_DIR}/TtlTargets.cmake")
endif()

# These are IMPORTED targets created by TtlTargets.cmake
set(TTL_LIBRARIES Ttl)
set(TTL_EXECUTABLE examples)
